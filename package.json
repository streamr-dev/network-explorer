    "axios": "^0.20.0",
    "core-js": "^3.8.0",
    "env-cmd": "^10.1.0",
    "history": "^5.0.0",
    "lodash": "^4.17.20",
    "mapbox-gl": "^1.12.0",
    "normalizr": "^3.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-identicons": "^1.2.4",
    "react-map-gl": "^5.3.15",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-spring": "^9.0.0",
    "react-virtuoso": "^2.2.2",
    "recharts": "^2.0.9",
    "streamr-client": "^5.4.4",
    "streamr-client-protocol": "^9.0.0",
    "streamr-client-react": "^2.0.0",
    "styled-components": "^5.1.1",
    "typescript": "^4.0.7"
  },
  "scripts": {
    "start": "BUILD_ENV=development npm run start-env",
    "start-env": "env-cmd -f .env.${BUILD_ENV} --no-override react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "eject": "react-scripts eject",
    "lint": "eslint --ext js,ts,tsx src --color",
    "format": "cross-env prettier --write 'src/**/*.{ts,tsx,css,json}'",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!*.stories.*"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 50
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.4.8",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/node-logger": "^6.0.21",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.21",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.54",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/recharts": "^2.0.0",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "3.10.0",
    "@typescript-eslint/parser": "3.10.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "7.0.2",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-typescript": "9.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-import-resolver-typescript": "2.2.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.0",
    "exports-loader": "^1.1.1",
    "prettier": "2.1.0",
    "prettier-eslint": "11.0.0",
    "prettier-eslint-cli": "5.0.0",
    "react-app-rewired": "^2.1.8",
    "react-is": "^16.13.1",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
